<h1>My tasks</h1>

<ul>
  <% @tasks.each do |task| %>
  <li>
    <%= link_to task.title, task_path(task), id: "task-id" %>
     <%= link_to "delete", task_path(task), method: :delete,
            data: { confirm: "Are you sure?" } %>
  </li>
  <% end  %>
</ul>

<%= link_to 'Add a task', tasks_new_path %>

